			BufferedImage image = ImageUtils.getBufferedImage("/home/deansmiller/Pictures/Jarvis Test Cases/Objects/101_ObjectCategories/BACKGROUND_Google/image_0171.jpg");
			BufferedWriter writer = new BufferedWriter(new FileWriter("/home/deansmiller/Pictures/Jarvis Test Cases/Hands/non_skin_pixels.data"));
			double[] input = new double[3];
			for(int w = 0; w < image.getWidth(); w++){
				for(int h = 0; h < image.getHeight(); h++){
					Color tmpColor = new Color(image.getRGB(w, h));
					input[0] = tmpColor.getRed();
					input[1] = tmpColor.getGreen();
					input[2] = tmpColor.getBlue();
					for(int i = 0; i < input.length; i++){
						writer.append(input[i] + ",");
					}
					writer.append(":0.0");
					writer.append("\n");
				}
			}
			
			writer.close();


